<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asha.chromelikeswipelayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScrollViewActivity" android:label="ScrollViewActivity"/>
        <activity android:name=".LinearLayoutActivity" android:label="LinearLayoutActivity"/>
        <activity android:name=".ListViewActivity" android:label="ListViewActivity"/>
        <activity android:name=".RecyclerViewActivity" android:label="RecyclerViewActivity"/>
        <activity android:name=".NestedRecyclerViewActivity" android:label="NestedRecyclerViewActivity"/>
        <activity android:name=".CustomHeightListViewActivity" android:label="CustomHeightListViewActivity"/>
        <activity android:name=".DisabledExampleActivity" android:label="DisabledExampleActivity"/>
    </application>

</manifest>
